// Including necessary header files

#include <bits/stdc++.h>
using namespace std;

// Class Solution
class Solution {
  public:
    // Recursive function to print numbers from 1 to n
    void printTillN(int n) {
        // Base case: If n becomes 0, return
        if (n == 0)
            return;

        // Recursively call the function with n-1
        printTillN(n - 1);

        // Print the current number
        cout << n << " ";
    }
};
